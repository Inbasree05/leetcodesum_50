int maxSubArray(int* nums, int numsSize) {
    int c_sum=nums[0];
    int m_sum=nums[0];
    for(int i=1;i<numsSize;i++){
        c_sum+=nums[i];
        if(c_sum<nums[i]){
            c_sum=nums[i];
        }
        else{
            c_sum=c_sum;
        }
        if(c_sum>m_sum){
            m_sum=c_sum;
        }
    }
    return m_sum;
}
